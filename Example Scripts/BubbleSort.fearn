fn main(args : str[]) => void
{
    let l : int[] = new int[] { 7, 6, 11, 23, 4, 1, 9 };

    print(bubble(l, 7));

}


fn bubble(l : int[], length : int) => int[]
{
    let swapped : bool = true;
    let temp : int;

    for (;swapped;)
    {
   		swapped = false;
   		for ( let i : int = 1; i < length; i += 1 )
   		{
            if ( l[i - 1] > l[i] )
   			{
                temp = l[i - 1];
                l[i - 1] = l[i];
                l[i] = temp;
                swapped = true;
   			}
   		}
    }

    return l;

}