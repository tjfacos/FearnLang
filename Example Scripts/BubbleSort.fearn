fn main() => void
    let
        l : int[7] = { 7, 6, 11, 23, 4, 1, 9 }
    
    print bubble(l)

fn bubble(l : int[]) => int[]
    let
        length : int = len(l)
        i : int = 1
        swapped : bool = True
        temp : int

    while swapped
        swapped = False
        while i < length:
            if l[i - 1] > l[i]
                temp = l[i - 1]
                l[i - 1] = l[i]
                l[i] = temp
                swapped = True
            i += 1
    
    return l