import io 
import random

let CHARSET : str = "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789!Â£$%^&*()+-_=@:,./?[]{}";

fn GeneratePassword(n : int) => str
{
    let password : str = "";
    for (let i : int = 0; i < n; i++)
    {
        password += CHARSET[randomInRange(0, CHARSET.length() - 1)];
    }
    return password;
}

fn main(args: str[]) => void
{
    print(GeneratePassword((int)args[0]));
}