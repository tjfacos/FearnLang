/* arithmetic.fearn : Demonstrates simple mathematical operations. */

import io

fn main() => void
{
    let a : float = (float)input("Enter A: ");
    let b : float = (float)input("Enter B: ");

    print("a + b : " + (str)(a + b));
    print("a - b : " + (str)(a - b));
    print("a * b : " + (str)(a * b));
    print("a / b : " + (str)(a / b));
    print("a ^ b : " + (str)(a ^ b));

    print("a % b : " + (str)((int)a % (int)b));

}