fn stalinSort(list : int[]) => int[]
{
    
    let temp_list : int[];
    let final_list : int[];
    let temp_ptr : int;
    let last : int;

    if (list.length() == 0)
    {
        return list;
    }

    temp_list = new int[list.length()];
    temp_ptr = 1;
    last = list[0];
    

    temp_list[0] = last;

    for (let i : int = 1; i < list.length(); i++)
    {
        if (list[i] >= last)
        {
            last = list[i];
            temp_list[temp_ptr++] = list[i];
        }
    }

    final_list = new int[temp_ptr];
    for (i = 0; i < temp_ptr; i++)
    {
        final_list[i] = temp_list[i];
    }

    return final_list;

}