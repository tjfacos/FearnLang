fn main(args : str[]) => void
{
    let n : int = (int)input("Enter n : ");
    print(fib(n));
}

fn fib(n : int) => int
{
    if (n == 0)
    {
        return 1;
    }
    
    if (n == 1)
    {
        return 1;
    } 
    
    return fib(n - 1) + fib(n - 2);
}


