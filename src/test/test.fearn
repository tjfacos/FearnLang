fn bubble(l : int[]) => int[]
{
    let length : int = len(l);
    let swapped : bool = True;
    let temp : int;
    let x : $MyStruct;

    for (;swapped;)
    {
        swapped = False;
        for (let i : int = 1; i < length; i += 1)
        {
