struct MyStruct
{
    let x : $MyOtherStruct;
}

struct MyOtherStruct
{
    let isTrue : bool;
}

fn main(args : str[]) => void
{
    let inst : $MyStruct = new MyStruct(
        new MyOtherStruct(true)
    );
    
    print(inst.x.isTrue);

    inst.x.isTrue = false;

    print(inst.x.isTrue);
}


